        <TextBlock x:Name="tbOut" HorizontalAlignment="Left" Margin="32,37,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Height="353" Width="625"/>
        <Button x:Name="btnRandom" Content="Generate Random" HorizontalAlignment="Left" Margin="154,9,0,0" VerticalAlignment="Top" Width="125" Height="23" Click="btnRandom_Click"/>
        <TextBox x:Name="txtSeed" HorizontalAlignment="Left" Height="23" Margin="34,9,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="btnMakeRoom" Content="Make Room" HorizontalAlignment="Left" Margin="681,17,0,0" VerticalAlignment="Top" Width="75" Click="btnMakeRoom_Click"/>
        <Button x:Name="btnShow" Content="Show" HorizontalAlignment="Left" Margin="681,60,0,0" VerticalAlignment="Top" Width="75" Click="btnShow_Click"/>

    </Grid>
</Window>
